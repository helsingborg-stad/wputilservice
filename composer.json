{
    "name": "helsingborg-stad/wputilservice",
    "description": "Simplifies WordPress development by providing a centralized WpUtilService that exposes a WordPress api wrapped in a streamlined manner. Simplify your development workflow and enhance WordPress integration with ease.",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "WpUtilService\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WpUtilService\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Sebastian Thulin",
            "email": "sebastian.nordqvistthulin@helsingborg.se"
        }
    ],
    "require": {
        "php": "^8.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^11",
        "php-stubs/wordpress-stubs": "^6.6",
        "johnpbloch/wordpress": "^6.6",
        "helsingborg-stad/wpservice": "^2",
        "carthage-software/mago": "^0.26.1",
        "phpstan/phpstan": "^2.1"
    },
    "scripts": {
        "test": "./vendor/bin/phpunit --testdox --no-coverage",
        "coverage": "XDEBUG_MODE=coverage ./vendor/bin/phpunit --testdox",
        "lint": "vendor/carthage-software/mago/composer/bin/mago lint",
        "fix": "vendor/carthage-software/mago/composer/bin/mago lint --format --fix",
        "stan": "php -d memory_limit=512M vendor/bin/phpstan analyse --configuration=phpstan.neon"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "johnpbloch/wordpress-core-installer": true,
            "carthage-software/mago": true
        }
    },
    "version": "0.2.15"
}